{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "../public",
    "allowJs": true,
    "isolatedModules": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["../static/renderer.ts"]
}
